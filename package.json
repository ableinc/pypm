{
    "name": "pypm2",
    "version": "0.1.3",
    "description": "Python package manager for projects running Python3.6 and above.",
    "main": "",
    "scripts": {
        "start": "python main.py",
        "test": "echo \"No test available\"",
        "install": "pip install --editable .",
        "build": "python setup.py sdist bdist_wheel",
        "publish": "twine upload dist/*"
    },
    "author": "AbleInc",
    "license": "ISC",
    "setup": {
        "name": "pypm2",
        "author": "AbleInc",
        "author_email": "douglas.jaylen@gmail.com",
        "description": "Python package manager for projects running Python3.6 and above.",
        "long_description": "file: README.md",
        "url": "https://github.com/ableinc/pypm",
        "include_package_data": true,
        "zip_safe": false,
        "keywords": [
            "package manager",
            "dependency manager",
            "manager",
            "python 3",
            "cli tool",
            "command line tool"
        ],
        "packages": [],
        "package_data": {
            "pypm": [
                "pypm/data/pkg.json",
                "pypm/data/setup.py",
                "pypm/data/setup.cfg",
                "pypm/data/pyproject.toml"
            ]
        },
        "data_files": [
            "pypm/data/pkg.json",
            "pypm/data/setup.py",
            "pypm/data/setup.cfg",
            "pypm/data/pyproject.toml"
        ],
        "entry_points": "\n[console_scripts]\npypm=pypm.cli:cli\n    ",
        "install_requires": [
            "Click==7.0",
            "stdlib-list==0.7.0"
        ],
        "classifiers": [
            "Programming Language :: Python :: 3",
            "License :: OSI Approved :: MIT License",
            "Operating System :: OS Independent"
        ]
    },
    "dependencies": {
